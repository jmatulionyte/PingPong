@model List<string>
@if (Model.Count != 0)
{
    <table class="table table-dark">
        <thead>
            <tr>

                <th>

                </th>

            </tr>
        </thead>
        <tbody>

            @foreach (var groupPlayer in Model)
            {
                <tr>

                    <td>
                        @groupPlayer
                    </td>
                </tr>

            }
        </tbody>
    </table>
}
